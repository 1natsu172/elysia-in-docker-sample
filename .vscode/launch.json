{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Docker node app",
      "type": "node",
      "request": "attach",
      "localRoot": "${workspaceFolder}",
      "remoteRoot": "/usr/src/app",
      "port": 9229,
      "restart": true,
      "skipFiles": [
        "<node_internals>/**",
        "${workspaceFolder}/node_modules/**",
      ],
    }
  ]
}